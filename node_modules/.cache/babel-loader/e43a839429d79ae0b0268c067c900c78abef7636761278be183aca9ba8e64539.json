{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, computed } from 'vue';\nexport default {\n  setup() {\n    const cardNumber = ref(['', '', '', '']);\n    const isValidCardNumber = computed(() => {\n      const flattened = cardNumber.value.join('');\n      if (flattened.length !== 16 || isNaN(flattened)) {\n        return false;\n      }\n      let sum = 0;\n      for (let i = 0; i < flattened.length; i++) {\n        let digit = parseInt(flattened[i], 10);\n        if ((i + 1) % 2 === 0) {\n          digit *= 2;\n          if (digit > 9) {\n            digit = digit - 9;\n          }\n        }\n        sum += digit;\n      }\n      return sum % 10 === 0;\n    });\n    const submitForm = () => {\n      if (!isValidCardNumber.value) {\n        alert(\"유효하지 않는 카드 번호 입니다.\");\n      } else {\n        console.log(\"제출 완료\");\n        this.router.push('/infoPage');\n      }\n    };\n    return {\n      cardNumber,\n      isValidCardNumber,\n      submitForm\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","setup","cardNumber","isValidCardNumber","flattened","value","join","length","isNaN","sum","i","digit","parseInt","submitForm","alert","console","log","router","push"],"sources":["/Users/kakao/Documents/balaan/src/components/finalStep.vue"],"sourcesContent":["<template>\n    <div>\n      <form @submit.prevent=\"submitForm\">\n        <div>\n          <label for=\"cardNumber\">카드번호</label>\n          <input v-model=\"cardNumber[0]\" maxlength=\"4\" required>\n          <input v-model=\"cardNumber[1]\" maxlength=\"4\" required>\n          <input v-model=\"cardNumber[2]\" maxlength=\"4\" required>\n          <input v-model=\"cardNumber[3]\" maxlength=\"4\" required>\n        </div>\n        <div>\n          <button :disabled=\"!isValidCardNumber\" type=\"submit\">완료</button>\n        </div>\n      </form>\n    </div>\n  </template>\n  \n  <script>\n  import { ref, computed } from 'vue';\n  \n  export default {\n    setup() {\n      const cardNumber = ref(['', '', '', '']);\n  \n      const isValidCardNumber = computed(() => {\n        const flattened = cardNumber.value.join('');\n        if (flattened.length !== 16 || isNaN(flattened)) {\n          return false;\n        }\n  \n        let sum = 0;\n        for (let i = 0; i < flattened.length; i++) {\n          let digit = parseInt(flattened[i], 10);\n  \n          if ((i + 1) % 2 === 0) {\n            digit *= 2;\n            if (digit > 9) {\n              digit = digit - 9;\n            }\n          }\n          sum += digit;\n        }\n        \n        return sum % 10 === 0;\n      });\n  \n      const submitForm = () => {\n        if (!isValidCardNumber.value) {\n          alert(\"유효하지 않는 카드 번호 입니다.\");\n        } else {\n          console.log(\"제출 완료\");\n          this.router.push('/infoPage') \n        }\n      };\n  \n      return {\n        cardNumber,\n        isValidCardNumber,\n        submitForm,\n      };\n    },\n  };\n  </script>\n  "],"mappings":";AAkBE,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AAEnC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAIH,GAAG,CAAC,CAAC,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,CAAC,CAAC;IAExC,MAAMI,iBAAgB,GAAIH,QAAQ,CAAC,MAAM;MACvC,MAAMI,SAAQ,GAAIF,UAAU,CAACG,KAAK,CAACC,IAAI,CAAC,EAAE,CAAC;MAC3C,IAAIF,SAAS,CAACG,MAAK,KAAM,EAAC,IAAKC,KAAK,CAACJ,SAAS,CAAC,EAAE;QAC/C,OAAO,KAAK;MACd;MAEA,IAAIK,GAAE,GAAI,CAAC;MACX,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIN,SAAS,CAACG,MAAM,EAAEG,CAAC,EAAE,EAAE;QACzC,IAAIC,KAAI,GAAIC,QAAQ,CAACR,SAAS,CAACM,CAAC,CAAC,EAAE,EAAE,CAAC;QAEtC,IAAI,CAACA,CAAA,GAAI,CAAC,IAAI,MAAM,CAAC,EAAE;UACrBC,KAAI,IAAK,CAAC;UACV,IAAIA,KAAI,GAAI,CAAC,EAAE;YACbA,KAAI,GAAIA,KAAI,GAAI,CAAC;UACnB;QACF;QACAF,GAAE,IAAKE,KAAK;MACd;MAEA,OAAOF,GAAE,GAAI,EAAC,KAAM,CAAC;IACvB,CAAC,CAAC;IAEF,MAAMI,UAAS,GAAIA,CAAA,KAAM;MACvB,IAAI,CAACV,iBAAiB,CAACE,KAAK,EAAE;QAC5BS,KAAK,CAAC,oBAAoB,CAAC;MAC7B,OAAO;QACLC,OAAO,CAACC,GAAG,CAAC,OAAO,CAAC;QACpB,IAAI,CAACC,MAAM,CAACC,IAAI,CAAC,WAAW;MAC9B;IACF,CAAC;IAED,OAAO;MACLhB,UAAU;MAC